<h2><%= l(:report_title_edit_daily) %></h2>
<% labelled_tabular_form_for :weeklydaily, @weeklydaily,
  :url => { :action => "update",:id => @weeklydaily.id },
  :html => { :class => nil, :multipart => true, :id => "weeklydaily-form" } do |form| %>
  <%= render :partial => 'form', :locals => { :f => form } %>
<p><%= submit_tag l(:report_button_edit) %><%= link_to l(:report_link_daily_back) ,:controller=>'report',:action => 'userindex' %></p>
<% end %>